# Get bash script location location
DIR="$( cd "$( dirname "$0" )" && pwd )"

name=g419-liner2-cli
jar=`find $DIR/$name/build/libs -name "$name*-all.jar" -type f`

if [ -f "$jar" ] 
then
  java -Xss4m -Xmx4024M -jar $jar $@
else
  echo "Jar file not found."
  echo "Run ./gradlew :$name:jar first."
fi