{
  "regexEscape": "%",
  "rules": {
    "age": {
      "extract": "$reAgeNumber%s+wiek[.]?",
      "normalize": "@group(0)"
    },
    "ageWrit": {
      "extract": "$reAgeWritten",
      "normalize": "@group(0)"
    },
    "ageOWR": {
      "extract": "wiek%s+$reAgeNumber",
      "normalize": "@group(0)"
    },
    "agePart": {
      "extract": "$reStart%s+$reAgeNumber%s+wiek[.]?",
      "normalize": "@group(1)"
    },
    "ageHalf": {
      "extract": "$reWhicHalf%s+$reHalf%s+$reAgeNumber%s+wiek[.]?",
      "normalize": "@group(2)-H@group(0)"
    }
  },
