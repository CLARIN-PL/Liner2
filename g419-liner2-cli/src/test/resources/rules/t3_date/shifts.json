{
  "regexEscape": "%",
  "rules": [
    {
      "tommorow": {
        "extract": "$reNext",
        "normalize": "+0000-00-01"
      }
    },
    {
      "dayAfterTommorow": {
        "extract": "pojutrze",
        "normalize": "+0000-00-02"
      }
    },
    {
      "dayBefore": {
        "extract": "$reBefore",
        "normalize": "-0000-00-01"
      }
    },
    {
      "preyesterday": {
        "extract": "przedwczoraj",
        "normalize": "-0000-00-02"
      }
    }
  ],
  "patterns": {
    "reBefore": [
      "dzień poprzedni",
      "poprzedni dzień",
      "wczoraj",
      "wczorajszy",
      "przeddzień"
    ],
    "reNext": [
      "dzień następny",
      "następny dzień",
      "nazajutrz",
      "jutro",
      "jutrzejszy"
    ]
  }
}