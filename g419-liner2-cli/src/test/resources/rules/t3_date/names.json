    ],
    "reDescription": [
      "początek",
      "środek",
      "koniec",
      "końcówka",
      "cały",
      "okres",
      "przedwczesny",
      "wczesny",
      "późny",
      "przesilenie",
      "długi"
    ],
    "rePart": [
      "początek",
      "środek",
      "koniec"
    ],
    "reMonth": [
      "styczeń",
      "styczen",
      "luty",
      "marzec",
      "kwiecień",
      "kwiecien",
      "maj",
      "czerwiec",
      "lipiec",
      "sierpień",
      "sierpien",
      "wrzesień",
      "wrzesien",
      "październik",
      "pazdziernik",
      "listopad",
      "grudzień",
      "grudzien",
      "stycznia",
      "lutego",
      "marca",
      "kwietnia",
      "maja",
      "czerwca",
      "lipca",
      "sierpnia",
      "września",
      "wrzesnia",
      "października",
      "pazdziernika",
      "listopada",
