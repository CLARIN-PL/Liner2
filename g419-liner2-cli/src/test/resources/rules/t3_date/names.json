  },
  "normalization": {
    "leadingZero": {
      "Wxx-01": "Wxx-1",
      "Wxx-02": "Wxx-2",
      "Wxx-03": "Wxx-3",
      "Wxx-04": "Wxx-4",
      "Wxx-05": "Wxx-5",
      "Wxx-06": "Wxx-6",
      "Wxx-07": "Wxx-7"
    },
    "dayOfWeek": {
      "poniedziałek": "1",
      "poniedziałkowy": "1",
      "poniedziałkowo": "1",
      "pon[.]?[^i]": "1",
      "wtorek": "2",
      "wtorkowy": "2",
      "wtorkowo": "2",
      "wt[.]?[^o]": "2",
      "środa": "3",
      "środowy": "3",
      "środowo": "3",
      "śr[.]?[^o]": "3",
      "czwartek": "4",
      "czwartkowy": "4",
      "czwartkowo": "4",
      "czw[.]?[^a]": "4",
      "piątek": "5",
      "piątkowy": "5",
      "piątkowo": "5",
      "pt[.]?": "5",
      "pod tytułem[.]": "5",
      "sobota": "6",
      "sobote": "6",
      "sobotni": "6",
      "sobotnio": "6",
      "sob[.]?[^o]": "6",
      "niedziela": "7",
