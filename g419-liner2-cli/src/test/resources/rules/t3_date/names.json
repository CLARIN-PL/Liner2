{
  "regexEscape": "%",
  "rules": {
    "today": {
      "extract": "$reToday",
      "normalize": "+0000-00-00"
    },
    "tomorrow": {
      "extract": "$reTomorrow",
      "normalize": "+0000-00-01"
    },
    "thisMonth": {
      "extract": "$reThisMonth",
      "normalize": "+0000-00"
    },
    "lastMonth": {
      "extract": "$reLastMonth",
      "normalize": "-0000-01"
    },
    "nextMonth": {
      "extract": "$reNextMonth",
      "normalize": "+0000-01"
    },
    "thisYear": {
      "extract": "$reThisYear",
      "normalize": "+0000"
    },
    "nextYear": {
      "extract": "$reNextYear",
      "normalize": "+0001"
    },
    "lastYear": {
      "extract": "$reLastYear",
      "normalize": "-0001"
    },
    "thisWeek": {
      "extract": "$reThisWeek",
      "normalize": "+0000-W00"
    },
    "nextWeek": {
      "extract": "$reNextWeek",
      "normalize": "+0000-W01"
    },
    "lastWeek": {
      "extract": "^reLastWeek$",
      "normalize": "-0000-W01"
    },
    "weekend": {
      "extract": "$reDescription?%s?$reWeekend",
      "normalize": "xxxx-Wxx-WE"
    },
    "thisWeekend": {
      "extract": "$reThisWeekend",
      "normalize": "+0000-W00-WE"
    },
    "nextWeekend": {
      "extract": "$reNextWeekend",
      "normalize": "+0000-W01-WE"
    },
    "monthWithDescription": {
      "extract": "$reDescription?%s?$reMonth",
      "normalize": "xxxx-@group(1)"
    },
    "monthThisYear": {
      "extract": "$reMonth%s+$thisYear",
      "normalize": "+0000-@group(1)"
    },
    "dayOfWeekWithDescription": {
      "extract": "$rePart?%s?+$reDayOfWeek",
      "normalize": "xxxx-Wxx-@group(1)"
    },
    "dayOfWeekWithDate": {
      "extract": "$reDayOfWeek%s$re2Digits%s$reMonth",
      "normalize": "xxxx-@group(2)-@group(1)"
    },
    "lastDayOfWeek": {
      "extract": "ostatni%s+$reDayOfWeek",
      "normalize": "<D@group(0)"
    },
    "nextDayOfWeek": {
      "extract": "przyszły%s+$reDayOfWeek",
      "normalize": ">D@group(0)"
    },
    "nextDayOfWeek1": {
      "extract": "bliski%s+$reDayOfWeek",
      "normalize": ">D@group(0)"
    },
    "nextDayOfWeek2": {
      "extract": "następny%s+$reDayOfWeek",
      "normalize": ">D@group(0)"
    },
    "partOfYear": {
      "extract": "$rePartOfYear",
      "normalize": "xxxx-@group(0)"
    },
    "thisPartOfYear": {
      "extract": "ten%s+$rePartOfYear",
      "normalize": "+0000-@group(0)"
    },
    "partOfYearWith4DigitsAndAdd": {
      "extract": "$reDescription?%s?$rePartOfYear%s$reCentury$re2Digits%s?$reYear?",
      "normalize": "@group(2)@group(3)-@group(1)"
    },
    "partOfYearWithDescription": {
      "extract": "$reDescription%s$rePartOfYear",
      "normalize": "xxxx-@group(1)"
    },
    "partOfYearWithLastYear": {
      "extract": "$rePartOfYear%s$reLastYear",
      "normalize": "xxxx-@group(0)"
    },
    "partOfYearWithThisYear": {
      "extract": "$rePartOfYear%s$reThisYear",
      "normalize": "xxxx-@group(0)"
    },
    "partOfYearWithNextYear": {
      "extract": "$rePartOfYear%s$reNextYear",
      "normalize": "xxxx-@group(0)"
    }
  },

  "patterns": {
    "reCentury": "[12]%d",
    "re2Digits": "%d%d",
    "reToday": [
      "dziś",
      "dzis",
      "dzisiaj",
      "dziś dzień",
      "ten dzień",
      "dzień ten",
      "dzień to",
      "dzisiejszy",
      "dzień dzisiejszy",
      "ten sam dzień"
    ],
    "reTomorrow": [
      "następny dzień",
      "nazajutrz"
    ],
    "reYesterday": [
      "przeddzień",
      "poprzedniego dnia"
    ],
    "reDayOfWeek": [
      "poniedziałek",
      "pon[.]?",
      "poniedziałkowy",
      "poniedziałkowo",
      "wtorek",
      "wt[.]?",
      "wtorkowy",
      "wtorkowo",
      "środa",
      "śr[.]?",
      "środowy",
      "środowo",
      "czwartek",
      "czwartkowy",
      "czwartkowo",
      "czw[.]?",
      "piątek",
      "piątkowy",
      "piątkowo",
      "pt[.]?",
      "pod tytułem[.]",
      "sobota",
      "sobote",
      "sobotni",
      "sobotnio",
      "sob[.]?",
      "niedziela",
      "niedzielny",
      "niedzielnie",
      "nd[.]?"
    ],
    "reYear": [
      "rok",
      "rocznik[.]"
    ],
    "reThisYear": [
      "ten rok",
      "ten sam rok",
      "bieżący rok",
      "rok bieżący",
      "tegoroczny"
    ],
    "reThisMonth": [
      "ten miesiąc"
    ],
    "reThisWeek": [
      "ten tydzień",
      "ten sam tydzień"
    ],
    "reNextWeekend": [
      "bliski weekend",
      "następny weekend",
      "przyszły weekend",
      "bliski weekend"
    ],
    "reNextYear": [
      "następny rok",
      "rok następny",
      "przyszły rok",
      "bliski rok",
      "rok przyszły"
    ],
    "reNextMonth": [
      "następny miesiąc",
      "miesiąc następny",
      "przyszły miesiąc",
      "bliski miesiąc",
      "miesiąc przyszły"
    ],
    "reNextWeek": [
      "następny tydzień",
      "przyszły tydzień"
    ],
    "reLastYear": [
      "zeszły rok",
      "zeszłoroczny",
      "ubiegły rok",
      "rok ubiegły",
      "ostatni rok"
    ],
    "reLastMonth": [
      "zeszły miesiąc",
      "zeszłomiesięczny",
      "ubiegły miesiąc",
      "miesiąc ubiegły",
      "ostatni miesiąc"
    ],
    "reLastWeek": [
